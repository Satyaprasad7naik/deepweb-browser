{
  "name": "deepweb-browser",
  "version": "1.0.0",
  "description": "An industry-grade deepweb browser with advanced privacy and security features",
  "main": "src/main/main.ts",
  "homepage": "https://github.com/Satyaprasad7naik/deepweb-browser",
  "author": "DeepWeb Browser Team",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "build": "electron-webpack",
    "package": "electron-builder --publish never",
    "package:win": "electron-builder --win",
    "package:mac": "electron-builder --mac",
    "package:linux": "electron-builder --linux",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "redux": "^4.2.0",
    "react-redux": "^8.0.5",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.3.2",
    "sqlite3": "^5.1.6",
    "crypto-js": "^4.1.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "node-tor": "^0.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-webpack": "^2.8.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "tailwindcss": "^3.2.6",
    "postcss": "^8.4.24",
    "autoprefixer": "^10.4.13",
    "jest": "^29.3.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "ts-jest": "^29.0.3",
    "prettier": "^2.8.2",
    "eslint": "^8.31.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
